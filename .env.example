# GeoSentinel Environment Configuration
# Copy this file to .env and fill in your actual API keys
# DO NOT commit .env file to git - add it to .gitignore

# News & Intelligence APIs
NEWS_API_KEY=your_newsapi_org_key_here
OPENROUTER_API_KEY=your_openrouter_ai_key_here

# Twitter/X API Keys (v2 API)
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_ACCESS_TOKEN=your_twitter_access_token
TWITTER_ACCESS_TOKEN_SECRET=your_twitter_access_token_secret
TWITTER_BEARER_TOKEN=your_twitter_bearer_token

# Geolocation & Infrastructure APIs
OPENCELLID_API_KEY=your_opencellid_api_key

# Hugging Face (for ML models)
HF_TOKEN=your_huggingface_token

# Generic API Key (fallback)
GENERIC_API_KEY=your_generic_api_key_here
